2		# number of conditions

gid		# geometric group identifier
IN		# condition type: inflow
5		# number of real parameters
Tt		# real parameter: total temperature
p		# real parameter: static pressure
1e0		# real parameter: direction of the flow, x component
0e0		# real parameter: direction of the flow, y component
0e0		# real parameter: direction of the flow, z component

gid		# geometric group identifier
OUT		# condition type: outflow
1		# number of real parameters
